<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.dss">

	<class name="org.openmrs.module.dss.hibernateBeans.Rule" table="dss_rule">
		
		<id name="ruleId" type="java.lang.Integer" column="rule_id">
			<generator class="native" />
		</id>
	
		<property name="classFilename" type="java.lang.String" column="class_filename"/>
		<property name="creationTime" type="java.util.Date" column="creation_time"/>
		<property name="priority" type="java.lang.Integer" column="priority" />
		<property name="title" type="java.lang.String" column="title"/>
		<property name="version" type="java.lang.Double" column="version"/>
		<property name="institution" type="java.lang.String" column="institution"/>
		<property name="author" type="java.lang.String" column="author"/>
		<property name="specialist" type="java.lang.String" column="specialist"/>
		<property name="ruleCreationDate" type="java.lang.String" column="ruleCreationDate"/>
		<property name="purpose" type="java.lang.String" column="purpose"/>
		<property name="explanation" type="java.lang.String" column="explanation"/>
		<property name="keywords" type="java.lang.String" column="keywords"/>
		<property name="citations" type="java.lang.String" column="citations"/>
		<property name="links" type="java.lang.String" column="links"/>
		<property name="logic" type="java.lang.String" column="logic"/>
		<property name="data" type="java.lang.String" column="data"/>
		<property name="action" type="java.lang.String" column="action"/>
		<property name="lastModified" type="java.util.Date" column="last_modified"/>
		<property name="ruleType" type="java.lang.String" column="rule_type"/>
		<property name="tokenName" type="java.lang.String" column="token_name"/>
		<property name="ageMinUnits" type="java.lang.String" column="age_min_units"/>
		<property name="ageMaxUnits" type="java.lang.String" column="age_max_units"/>
		<property name="ageMin" type="java.lang.Integer" column="age_min" />
		<property name="ageMax" type="java.lang.Integer" column="age_max" />
	</class>
	
	<class name="org.openmrs.module.dss.hibernateBeans.RuleAttribute" table="dss_rule_attribute">
		
		<id name="ruleAttributeId" type="java.lang.Integer" column="rule_attribute_id">
			<generator class="native" />
		</id>
	
		<property name="name" type="java.lang.String" column="name"/>
		<property name="description" type="java.lang.String" column="description"/>
	</class>
	
	<class name="org.openmrs.module.dss.hibernateBeans.RuleAttributeValue" table="dss_rule_attribute_value">
		
		<id name="ruleAttributeValueId" type="java.lang.Integer" column="rule_attribute_value_id">
			<generator class="native" />
		</id>
	
		<property name="ruleId" type="java.lang.Integer" column="rule_id"/>
		<property name="value" type="java.lang.String" column="value"/>
		<property name="ruleAttributeId" type="java.lang.Integer" column="rule_attribute_id"/>
		<property name="creationTime" type="java.util.Date" column="creation_time"/>
		<property name="location_tag_id" type="java.lang.Integer" column="locationTagId"/>
		<property name="location_id" type="java.lang.Integer" column="locationId"/>
		<property name="lastModified" type="java.util.Date" column="last_modified"/>
		
	</class>
	
</hibernate-mapping>
